{
  "name": "wm-product-search-ui",
  "version": "0.0.0",
  "private": true,
  "dependencies": {
    "@angular/animations": "6.0.7",
    "@angular/cdk": "8.0.1",
    "@angular/common": "8.0.3",
    "@angular/compiler": "8.0.3",
    "@angular/core": "8.0.3",
    "@angular/fire": "5.2.1",
    "@angular/forms": "8.0.3",
    "@angular/http": "7.2.16",
    "@angular/platform-browser": "8.0.3",
    "@angular/platform-browser-dynamic": "8.0.3",
    "@angular/router": "8.0.3",
    "@ng-bootstrap/ng-bootstrap": "5.0.0-rc.1",
    "angular-in-memory-web-api": "0.8.0",
    "bootstrap": "4.1.1",
    "core-js": "2.6.9",
    "hammerjs": "2.0.8",
    "rxjs": "6.5.2",
    "rxjs-compat": "^6.5.2",
    "tslib": "1.10.0",
    "web-animations-js": "2.3.2",
    "zone.js": "0.9.1"
  },
  "scripts": {
    "ng": "ng",
    "start": "sleep 1 && ng serve",
    "build": "ng build",
    "start-dist": "superstatic dist --port 8080 --project=\"wm-product-search-ui\" --host 127.0.0.1",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "ng lint"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.10.0",
    "@angular/cli": "~7.0.2",
    "@angular/compiler-cli": "~7.0.0",
    "@angular/language-service": "~7.0.0",
    "@types/node": "~10.7.1",
    "@types/jest": "^22.2.3",
    "codelyzer": "~4.5.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "jasmine-core": "~2.99.1",
    "jasmine-only": "^0.1.2",
    "jasmine-spec-reporter": "~4.2.1",
    "jest": "^24.5.0",
    "jest-canvas-mock": "^2.1.0",
    "jest-each": "^24.9.0",
    "jest-preset-angular": "^7.0.1",
    "mockery": "2.1.0",
    "node-sass": "^4.5.0",
    "phantomjs-polyfill": "0.0.2",
    "prettier": "^1.14.2",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.1",
    "tslint": "5.11.0",
    "tslint-config-prettier": "^1.15.0",
    "tslint-config-standard": "^8.0.0",
    "tslint-sonarts": "^1.8.0",
    "typescript": "~3.1.1",
    "superstatic": "^6.0.1"
  },
  "jest": {
    "preset": "jest-preset-angular",
    "setupFilesAfterEnv": [
      "jest-canvas-mock",
      "<rootDir>/src/test-config/setupJest.ts"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(cropperjs)/)"
    ],
    "moduleNameMapper": {
      "@core/(.*)": "<rootDir>/src/app/core/$1",
      "@shared/(.*)": "<rootDir>/src/app/shared/$1",
      "@modules/(.*)": "<rootDir>/src/app/shared/$1",
      "@environment/(.*)": "<rootDir>/src/environments/$1",
      "@mocks/(.*)": "<rootDir>/mocks/$1"
    },
    "collectCoverageFrom": [
      "src/app/**/*.ts",
      "!<rootDir>/node_modules*",
      "!<rootDir>/dist/",
      "!<rootDir>/src/app/app-shell/*"
    ],
    "testPathIgnorePatterns": [
      "src/test.ts",
      "/node_modules/",
      "/dist/",
      "src/app/*.{js}"
    ]
  },
  "globals": {
    "ts-jest": {
      "tsConfig": "<rootDir>/src/tsconfig.spec.json",
      "diagnostics": false,
      "stringifyContentPathRegex": "\\.html$",
      "astTransformers": [
        "jest-preset-angular/InlineHtmlStripStylesTransformer.js"
      ]
    },
    "window": true
  }
}
